<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ServletDemo</servlet-name>
    <servlet-class>pl.politechnika.ServletDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CalcServlet</servlet-name>
    <servlet-class>pl.politechnika.calc.CalcServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PollServlet</servlet-name>
    <servlet-class>pl.politechnika.poll.PollServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PollExcelServlet</servlet-name>
    <servlet-class>pl.politechnika.poll.excel.PollExcelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PollChartServlet</servlet-name>
    <servlet-class>pl.politechnika.poll.chart.PollChartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RedirectServlet</servlet-name>
    <servlet-class>pl.politechnika.RedirectServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CalcServlet</servlet-name>
    <url-pattern>/calc</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDemo</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PollServlet</servlet-name>
    <url-pattern>/poll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PollExcelServlet</servlet-name>
    <url-pattern>/poll/excel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PollChartServlet</servlet-name>
    <url-pattern>/poll/chart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RedirectServlet</servlet-name>
    <url-pattern>/redirect</url-pattern>
  </servlet-mapping>

</web-app>
